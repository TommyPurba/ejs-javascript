<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    <div class="box">
            
            <% for(let i =0;i<newlistItem.length; i++){ %>
                <div class="item">
                    <input type="checkbox">
                    <p><%=newlistItem[i]%><p></p>
                </div>  
            <% } %>

            <form class="item" action="/" method="post">
                <input type="text" name="isi" autocomplete="off" placeholder="New Item">
                <button type="submit" name="list" value=<%= listTitle %>>+</button>
            </form>
        
    </div>
   
    <%- include("footer") -%>


